{
  "name": "cspray/marked-logs",
  "description": "A PSR-11 Logger that will ensure all logs have their context marked with an identifier.",
  "keywords": ["logger"],
  "license": ["MIT"],
  "require": {
    "php": "^8.1",
    "psr/log": "^3.0"
  },
  "require-dev": {
    "monolog/monolog": "^3.3",
    "roave/security-advisories": "dev-latest",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Cspray\\MarkedLogs\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cspray\\MarkedLogs\\Test\\": "tests"
    }
  }
}